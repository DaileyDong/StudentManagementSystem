<%include header%>
<div class="table-responsive">
        <table class="table table-hover table-condensed text-center">
                <tr>
                    <td><h4>学号</h4></td>
                    <td><h4>姓名</h4></td>
                    <td><h4>年龄</h4></td>
                    <td><h4>性别</h4></td>
                    <td><h4>专业</h4></td>
                    <td><h4>操作</h4></td>
                </tr>
                <%docs.forEach(function (doc) {%>
                <tr class="main">
                    <td><h4><%=doc.studentID%></h4></td>
                    <td><h4><%=doc.name%></h4></td>
                    <td><h4><%=doc.age%></h4></td>
                    <td><h4><%=doc.sex%></h4></td>
                    <td><h4><%=doc.profession%></h4></td>
                    <td><a class="edit" href="/edit/<%=doc.name%>/<%=doc.sex%>/<%=doc.studentID%>" data-toggle="modal">编辑</a>
                        <a class="remove" href="/remove/<%=doc.name%>/<%=doc.sex%>/<%=doc.studentID%>">删除</a></td>
                </tr>
                <%})%>
            </table>
</div>

<%include footer%>